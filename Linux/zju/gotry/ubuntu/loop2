cat > namelist <<EOF
kevin
zhangd
fengmei
yerui
xieteng
zhangmj
songyi
suxc
menglj
hezhi
alcohol
suyx
fuyyzju
yanlei
huangjx
flei
laoyc
zhouhong
zhong
zgq
tanhaojing
jiangyw
EOF

for ii in `cat namelist`; do
    unlink /data/$ii/$ii
    unlink /home/$ii/data
    if [[ -d /data/$ii ]]; then 
        ln -s /data/$ii /home/$ii/data
    else
        mkdir -p /data/$ii
        chmod 700 /data/$ii
        chown $ii:$ii /data/$ii
        ln -s /data/$ii/ /home/$ii/data
    fi
done
rm namelist